{
    "name": "ZingUI",
    "version": "0.2.0",
    "author": "Mathias Bergqvist",
    "homepage": "http://fallacious-stretch.surge.sh/",
    "private": true,
    "license": "MIT",
    "description": "Zing Dashboard an ebusiness manager",
    "scripts": {
        "build:mockapi": "babel-node test/mockdb.js",
        "mockapi": "json-server --watch www/db.json --port 4001",
        "security-check": "nsp check",
        "clean": "ionic-app-scripts clean",
        "build": "npm run ionic:build",
        "ionic:build": "ionic-app-scripts build --prod",
        "ionic:serve": "ionic-app-scripts serve",
        "ionic:serve:e2e": "ionic serve --nobrowser --port 4200",
        "protractor": "protractor",
        "e2e": "npm-run-all --parallel ionic:serve:e2e protractor",
        "postinstall": "ionic cordova prepare && webdriver-manager update",
        "start": "ionic serve",
        "test": "ng test",
        "webdriver": "webdriver-manager start",
        "pretest:cucumber": "rimraf out-tsc && tsc -p tsconfig.cucumber.json",
        "test:cucumber": "protractor out-tsc/protractor.cucumber.conf.js",
        "test-coverage": "ng test --code-coverage",
        "test-ci": "ng test --watch=false --code-coverage"
    },
    "dependencies": {
        "@angular/common": "4.1.3",
        "@angular/compiler": "4.1.3",
        "@angular/compiler-cli": "4.1.3",
        "@angular/core": "4.1.3",
        "@angular/forms": "4.1.3",
        "@angular/http": "4.1.3",
        "@angular/platform-browser": "4.1.3",
        "@angular/platform-browser-dynamic": "4.1.3",
        "@angular/router": "4.1.3",
        "@ionic-native/core": "3.10.2",
        "@ionic-native/splash-screen": "3.10.2",
        "@ionic-native/status-bar": "3.10.2",
        "@ionic/storage": "2.0.1",
        "babel-cli": "^6.24.1",
        "babel-preset-angular2": "0.0.2",
        "babel-preset-env": "^1.6.0",
        "babel-preset-es2015": "^6.24.1",
        "chalk": "^2.0.1",
        "cordova-android": "^5.1.1",
        "cordova-browser": "^4.0.0",
        "cordova-ios": "^4.0.1",
        "cordova-plugin-console": "^1.0.5",
        "cordova-plugin-device": "^1.1.4",
        "cordova-plugin-splashscreen": "^4.0.3",
        "cordova-plugin-statusbar": "^2.2.2",
        "cordova-plugin-whitelist": "^1.3.1",
        "ionic-angular": "3.4.2",
        "ionic-plugin-keyboard": "^2.2.1",
        "ionicons": "3.0.0",
        "npm-run-all": "^4.0.2",
        "rimraf": "^2.6.1",
        "rxjs": "5.4.0",
        "sw-toolbox": "3.6.0",
        "zone.js": "0.8.12"
    },
    "devDependencies": {
        "@angular/cli": "1.1.2",
        "@ionic/app-scripts": "1.3.7",
        "@ionic/cli-plugin-cordova": "1.5.0",
        "@ionic/cli-plugin-ionic-angular": "1.4.0",
        "@types/cucumber": "^2.0.1",
        "@types/jasmine": "2.5.41",
        "@types/node": "7.0.4",
        "@types/selenium-webdriver": "^3.0.4",
        "chai": "^4.1.0",
        "chai-as-promised": "^7.1.1",
        "codecov": "2.2.0",
        "codelyzer": "^3.1.2",
        "connect": "3.6.2",
        "cucumber": "^2.3.1",
        "cucumber-html-reporter": "^2.0.3",
        "fs": "0.0.1-security",
        "git-scripts": "^0.2.1",
        "ionic": "3.6.0",
        "jasmine": "^2.7.0",
        "jasmine-core": "2.6.2",
        "jasmine-spec-reporter": "4.1.0",
        "json-schema-faker": "^0.4.3",
        "json-server": "^0.11.2",
        "karma": "1.7.0",
        "karma-chrome-launcher": "2.1.1",
        "karma-cli": "1.0.1",
        "karma-coverage-istanbul-reporter": "1.2.1",
        "karma-jasmine": "1.1.0",
        "karma-jasmine-html-reporter": "0.2.2",
        "localtunnel": "^1.8.3",
        "nsp": "^2.7.0",
        "protractor": "5.1.2",
        "protractor-cucumber-framework": "^3.1.2",
        "serve-static": "1.12.3",
        "surge": "^0.19.0",
        "ts-node": "3.0.4",
        "tslint": "5.3.2",
        "tslint-eslint-rules": "4.1.1",
        "typescript": "2.3.3"
    },
    "cordovaPlugins": [
        "cordova-plugin-whitelist",
        "cordova-plugin-console",
        "cordova-plugin-statusbar",
        "cordova-plugin-device",
        "cordova-plugin-splashscreen",
        "ionic-plugin-keyboard"
    ],
    "cordovaPlatforms": [],
    "repository": {
        "type": "git",
        "url": "https://github.com/mathiasb/z3-ui.git"
    },
    "cordova": {
        "platforms": [
            "android",
            "browser",
            "ios"
        ],
        "plugins": {
            "cordova-plugin-console": {},
            "cordova-plugin-device": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-whitelist": {},
            "ionic-plugin-keyboard": {}
        }
    },
    "git": {
        "scripts": {
            "pre-push": "surge --project ./www --domain fallacious-stretch.surge.sh"
        }
    }
